<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Skills</title>
</head>
<body>
    <h1>Skills</h1>
    <ul>
        <% skills.forEach(function(s){ %>
            <li>
                <form action="/skills/<%= s.skill%>?_method=DELETE"
                class="delete-form" method="POST">
                    <button type="submit">X</button>
                </form>
                <a href="/skills/<%= s.skill %>"><%= s.skill %></a>
            </li>
        <% }); %>
    </ul>
    <a href="/">home</a><strong> | </strong><a href="/skills/new">add skill</a>
</body>
</html>